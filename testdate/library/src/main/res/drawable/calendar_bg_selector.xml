<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


  <item app:tsquare_state_range_middle="true">

    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
      <item android:drawable="@drawable/border"/>
      <item android:drawable="@drawable/innerrange"
          android:top="7dp"
          android:right="0dp"
          android:bottom="7dp"
          android:left="0dp" />
    </layer-list>
  </item>


  <item android:state_selected="true">

    <shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval">

      <size android:height="1dp"
          android:width="1dp" />
      <stroke android:color="#00FFFFFF"
          android:width="10dp"/>
      <solid android:color="#b9f5082b"/>
    </shape>
  </item>

  <item app:tsquare_state_range_first="true">

    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
      <item android:drawable="@drawable/border"/>
      <item android:drawable="@drawable/innerselectedstart"
          android:top="7dp"
          android:right="0dp"
          android:bottom="7dp"
          android:left="7dp" />
    </layer-list>
  </item>

  <item app:tsquare_state_range_last="true">

    <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
      <item android:drawable="@drawable/border"/>
      <item android:drawable="@drawable/innerselectedend"
          android:top="7dp"
          android:right="7dp"
          android:bottom="7dp"
          android:left="0dp" />
    </layer-list>
  </item>


  <item android:state_pressed="true">

    <shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval">

      <size android:height="1dp"
          android:width="1dp" />
      <stroke android:color="#00FFFFFF"
          android:width="10dp"/>
      <solid android:color="#b9f5082b"/>
    </shape>
  </item>

  <!--<item app:tsquare_state_range_same="true">-->

    <!--<shape xmlns:android="http://schemas.android.com/apk/res/android"-->
        <!--android:shape="oval">-->

      <!--<size android:height="1dp"-->
          <!--android:width="1dp" />-->
      <!--<stroke android:color="#00FFFFFF"-->
          <!--android:width="10dp"/>-->
      <!--<solid android:color="#b9f5082b"/>-->
    <!--</shape>-->
  <!--</item>-->


  <item app:tsquare_state_current_month="false">
    <color android:color="@color/calendar_inactive_month_bg"/>
  </item>
  <item app:tsquare_state_highlighted="true">
    <color android:color="@color/calendar_highlighted_day_bg"/>
  </item>
  <item app:tsquare_state_today="true">

    <shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval">

      <size android:height="1dp"
          android:width="1dp" />
      <stroke android:color="#00FFFFFF"
          android:width="10dp"/>
      <solid android:color="#9292d1f2"/>
    </shape>

    <!--<color android:color="@color/calendar_today"/>-->
  </item>
  <item>
    <color android:color="@color/calendar_active_month_bg"/>
  </item>
</selector>
